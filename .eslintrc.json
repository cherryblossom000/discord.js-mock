{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "accessor-pairs": [2, {"enforceForClassMembers": true}],
    "array-bracket-spacing": 2,
    "arrow-spacing": 2,
    "block-spacing": [2, "never"],
    "brace-style": 2,
    "camelcase": 2,
    "comma-dangle": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": [2, "never", {"enforceForClassMembers": true}],
    "curly": [2, "multi-or-nest"],
    "dot-location": [2, "property"],
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": [2, "smart"],
    "func-call-spacing": 2,
    "generator-star-spacing": [2, {"before": false, "after": true}],
    "indent": [2, 2, {
      "SwitchCase": 1,
      "ignoredNodes": ["TemplateLiteral *"]
    }],
    "key-spacing": 2,
    "keyword-spacing": 2,
    "lines-between-class-members": 2,
    "max-len": [2, 125],
    "max-statements-per-line": 2,
    "new-cap": 2,
    "new-parens": [2, "always"],
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-dupe-class-members": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-parens": 2,
    "no-fallthrough": 0,
    "no-implied-eval": 2,
    "no-inner-declarations": [2, "both"],
    "no-irregular-whitespace": [2, {"skipTemplates": true}],
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": [2, {
      "max": 2,
      "maxEOF": 2,
      "maxBOF": 0
    }],
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 2,
    "no-path-concat": 2,
    "no-proto": 2,
    "no-return-await": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-tabs": 2,
    "no-template-curly-in-string": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-unmodified-loop-condition": 2,
    "no-unneeded-ternary": 2,
    "no-unused-expressions": [2, {
      "allowShortCircuit": true,
      "allowTernary": true,
      "allowTaggedTemplates": true
    }],
    "no-unused-vars": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-useless-call": 2,
    "no-useless-computed-key": 2,
    "no-useless-constructor": 2,
    "no-useless-rename": 2,
    "no-useless-return": 2,
    "no-void": 2,
    "no-whitespace-before-property": 2,
    "nonblock-statement-body-position": 2,
    "object-curly-newline": [2, {
      "multiline": true,
      "consistent": true
    }],
    "object-curly-spacing": 2,
    "object-property-newline": [2, {"allowAllPropertiesOnSameLine": true}],
    "one-var": [2, {"initialized": "never"}],
    "operator-linebreak": [2, "after", {"overrides": {
      "?": "before",
      ":": "before",
      "|>": "before"
    }}],
    "padded-blocks": [2, "never"],
    "prefer-const": 2,
    "prefer-promise-reject-errors": 2,
    "quote-props": [2, "as-needed"],
    "quotes": 2,
    "rest-spread-spacing": 2,
    "semi": 2,
    "semi-spacing": [2, {"before": false, "after": true}],
    "space-before-blocks": 2,
    "space-before-function-paren": [2, "never"],
    "space-in-parens": 2,
    "space-infix-ops": [2, {"int32Hint": true}],
    "space-unary-ops": [2, {
      "words": true,
      "nonwords": false
    }],
    "spaced-comment": [2, "always", {
      "line": {"markers": ["/"]},
      "block": {"markers": ["*"], "balanced": true}
    }],
    "symbol-description": 2,
    "template-curly-spacing": 2,
    "template-tag-spacing": 2,
    "unicode-bom": 2,
    "wrap-iife": [2, "inside"],
    "yield-star-spacing": 2,
    "yoda": 2
  }
}
